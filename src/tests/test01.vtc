varnishtest "Test jemalloc vmod logging"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import jemalloc from "${vmod_topbuild}/src/.libs/libvmod_jemalloc.so";

	sub vcl_deliver {
		jemalloc.print_stats("");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.status == 200
}

client c1 -run
